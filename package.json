{
  "name": "rebabel-webpack-plugin",
  "version": "0.1.1",
  "description": "A webpack plugin that let you re-transpile asset JS files with babel and save to another file",
  "main": "index.es5.js",
  "jsnext:main": "index.js",
  "module": "index.js",
  "scripts": {
    "build": "webpack",
    "prepublish": "npm run transpile",
    "transpile": "babel index.js --out-file index.es5.js --no-babelrc --presets=es2015"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "babel-core": "^6.25.0"
  },
  "dependencies": {
    "webpack-sources": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "webpack": "^3.0.0"
  }
}
